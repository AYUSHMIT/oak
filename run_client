#!/bin/sh
set -e
set -x

./build_client

readonly EXAMPLE_NAME=hello_world
#readonly EXAMPLE_NAME=machine_learning
#readonly EXAMPLE_NAME=millionaires

./bazel-bin/oak/client/client \
  --scheduler_address=127.0.0.1:8888 \
  --module=$PWD/rust/oak/examples/$EXAMPLE_NAME/target/wasm32-unknown-unknown/release/$EXAMPLE_NAME.wasm
